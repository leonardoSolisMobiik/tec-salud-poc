# TecSalud Frontend Environment Configuration

# API Configuration
REACT_APP_API_URL=http://localhost:8000/api/v1
REACT_APP_API_TIMEOUT=30000

# Application Configuration
REACT_APP_NAME=TecSalud
REACT_APP_VERSION=2.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_VOICE_ASSISTANT=true
REACT_APP_ENABLE_PDF_VIEWER=true
REACT_APP_ENABLE_DOCUMENT_UPLOAD=true
REACT_APP_ENABLE_ANALYTICS=false

# Azure OpenAI Configuration (for frontend display only)
REACT_APP_AZURE_OPENAI_MODELS=gpt-4o,gpt-4o-mini

# Debug Configuration
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=info

