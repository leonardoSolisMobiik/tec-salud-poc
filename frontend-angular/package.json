{
  "name": "frontend-angular",
  "version": "1.0.0",
  "description": "TecSalud Medical Assistant - Angular frontend application for intelligent medical document management and chat functionality",
  "author": "TecSalud Development Team",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --port 4200 --open --hmr --configuration development",
    "start": "ng serve --port 4200 --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot --optimization",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@ti-tecnologico-de-monterrey-oficial/ds-ng": "^1.5.5",
    "marked": "^15.0.12",
    "ng2-pdf-viewer": "^10.4.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.6.0",
    "typescript": "~5.7.2"
  }
